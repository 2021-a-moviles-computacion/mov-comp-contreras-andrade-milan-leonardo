<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".actualizarUsuario">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tituloActivityActualizarUsuario"
        app:layout_constraintBottom_toTopOf="@+id/ti_actualizar_cedula"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.4" />

    <Button
        android:id="@+id/btn_actualizar_Usuario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="148dp"
        android:text="@string/botonActualizarUsuario"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/ti_actualizar_cedula"
        android:layout_width="290dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="40dp"
        android:clickable="false"
        android:editable="false"
        android:ems="10"
        android:hint="@string/input_cedula"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/ti_actualizar_nombre"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/ti_actualizar_nombre"
        android:layout_width="290dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="40dp"
        android:ems="10"
        android:hint="@string/input_nombre"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/ti_actualizar_apellido"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/ti_actualizar_apellido"
        android:layout_width="290dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="40dp"
        android:ems="10"
        android:hint="@string/input_apellido"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/ti_actualizar_telefono"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/ti_actualizar_telefono"
        android:layout_width="290dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="40dp"
        android:ems="10"
        android:hint="@string/input_telefono"
        android:inputType="phone"
        app:layout_constraintBottom_toTopOf="@+id/ti_actualizar_fechaNacimiento"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/ti_actualizar_fechaNacimiento"
        android:layout_width="290dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="40dp"
        android:ems="10"
        android:hint="@string/input_fechaNacimiwnto"
        android:inputType="date"
        app:layout_constraintBottom_toTopOf="@+id/btn_actualizar_Usuario"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="4dp"
        android:text="@string/input_cedula"
        app:layout_constraintBottom_toTopOf="@+id/ti_actualizar_cedula"
        app:layout_constraintEnd_toEndOf="@+id/ti_actualizar_cedula"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/ti_actualizar_cedula" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/input_nombre"
        app:layout_constraintBottom_toTopOf="@+id/ti_actualizar_nombre"
        app:layout_constraintEnd_toEndOf="@+id/ti_actualizar_nombre"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/ti_actualizar_nombre" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/input_apellido"
        app:layout_constraintBottom_toTopOf="@+id/ti_actualizar_apellido"
        app:layout_constraintEnd_toEndOf="@+id/ti_actualizar_apellido"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/ti_actualizar_apellido" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/input_telefono"
        app:layout_constraintBottom_toTopOf="@+id/ti_actualizar_telefono"
        app:layout_constraintEnd_toEndOf="@+id/ti_actualizar_telefono"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/ti_actualizar_telefono" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/input_fechaNacimiwnto"
        app:layout_constraintBottom_toTopOf="@+id/ti_actualizar_fechaNacimiento"
        app:layout_constraintEnd_toEndOf="@+id/ti_actualizar_fechaNacimiento"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/ti_actualizar_fechaNacimiento" />
</androidx.constraintlayout.widget.ConstraintLayout>